{
  "name": "elm-i18next-gen",
  "version": "1.0.0",
  "description": "Code generation for elm-i18next",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/yonigibbs/elm-i18next-gen"
  },
  "scripts": {
    "test": "mocha",
    "test-watch": "mocha --watch",
    "lint": "eslint **/*.js",
    "lint-fix": "eslint --fix **/*.js",
    "elm-format-validate": "mkdir -p elm-format-validate && bin/elm-i18next-gen -s test/resources/sample.json -t elm-format-validate -o && npx elm-format elm-format-validate/ --validate"
  },
  "author": "",
  "license": "ISC",
  "bin": {
    "elm-i18next-gen": "bin/elm-i18next-gen"
  },
  "devDependencies": {
    "@types/chai": "^4.2.7",
    "@types/command-line-args": "^5.0.0",
    "@types/mocha": "^5.2.7",
    "chai": "^4.2.0",
    "elm": "^0.19.1-3",
    "elm-format": "^0.8.2",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "mocha": "^7.0.0"
  },
  "dependencies": {
    "command-line-args": "^5.1.1",
    "fs-extra": "^8.1.0"
  }
}
